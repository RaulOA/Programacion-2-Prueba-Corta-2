<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>productos_Servlet</servlet-name>
        <servlet-class>pck.productos.productos_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>insertar_Producto_Servlet</servlet-name>
        <servlet-class>pck.productos.insertar_Producto_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>eliminar_Producto_Servlet</servlet-name>
        <servlet-class>pck.productos.eliminar_Producto_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cargar_Producto_Servlet</servlet-name>
        <servlet-class>pck.productos.cargar_Producto_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>actualizar_Producto_Servlet</servlet-name>
        <servlet-class>pck.productos.actualizar_Producto_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editar_Producto_Servlet</servlet-name>
        <servlet-class>pck.productos.editar_Producto_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>modificar_Producto_Servlet</servlet-name>
        <servlet-class>pck.productos.modificar_Producto_Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>productos_Servlet</servlet-name>
        <url-pattern>/productos_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>insertar_Producto_Servlet</servlet-name>
        <url-pattern>/insertar_Producto_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>eliminar_Producto_Servlet</servlet-name>
        <url-pattern>/eliminar_Producto_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cargar_Producto_Servlet</servlet-name>
        <url-pattern>/cargar_Producto_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>actualizar_Producto_Servlet</servlet-name>
        <url-pattern>/actualizar_Producto_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editar_Producto_Servlet</servlet-name>
        <url-pattern>/editar_Producto_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>modificar_Producto_Servlet</servlet-name>
        <url-pattern>/modificar_Producto_Servlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
